{
    "php.validate.executablePath": "/usr/bin/php",
    "editor.hideCursorInOverviewRuler": true,
    "editor.lineHeight": 25,
    "editor.matchBrackets": "always",
    // "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontFamily": "SauceCodePro Nerd Font",
    "editor.occurrencesHighlight": true,
    "editor.overviewRulerBorder": false,
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "all",
    "editor.folding": true,
    "editor.snippetSuggestions": "top",
    "editor.minimap.enabled": false,
    "editor.cursorBlinking": "solid",
    "editor.lineNumbers": "on",
    "editor.cursorStyle": "line",
    "search.smartCase": true,
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.idea": true,
        "**/.vscode": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "vendor": true,
        "package-lock.json": false
    },
    "explorer.openEditors.visible": 0,
    "explorer.confirmDragAndDrop": false,
    "code-runner.clearPreviousOutput": true,
    "code-runner.executorMap": {
        "python": "python3"
    },
    "editor.formatOnSave": true,
    "code-runner.runInTerminal": false,
    "sync.gist": "a6aff7efa62b3d6f324b08bdf122c9d5",
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.forceDownload": false,
    "sync.quietSync": false,
    "editor.fontSize": 16,
    "files.associations": {
        "*.toml": "toml"
    },
    "window.zoomLevel": -1,
    "window.title": "${activeEditorMedium}",
    "window.newWindowDimensions": "maximized",
    "workbench.activityBar.visible": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.showIcons": false,
    "typescript.check.npmIsInstalled": false,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "javascript.updateImportsOnFileMove.enabled": "never",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": false
    },
    "gitlens.codeLens.enabled": false,
    "gitlens.keymap": "chorded",
    "gitlens.codeLens.scopes": [
        "containers",
        "document"
    ],
    "sync.removeExtensions": true,
    "sync.syncExtensions": true,
    "extensions.autoUpdate": true,
    "editor.multiCursorModifier": "alt",
    "workbench.list.horizontalScrolling": true,
    "php-docblocker.extra": [
        "@author Twor",
        "@since $CURRENT_YEAR $CURRENT_MONTH $CURRENT_DATE"
    ],
    "workbench.colorCustomizations": {
        "titleBar.activeBackground": "#1e1e1e",
        "activityBar.background": "#1e1e1e",
        "tab.inactiveBackground": "#1e1e1e",
        "tab.unfocusedHoverBackground": "#747476",
        "tab.activeBackground": "#747476",
        "sideBar.background": "#1e1e1e",
        "sideBarSectionHeader.border": "#0A0A0D",
        "sideBarSectionHeader.background": "#1e1e1e",
        "menubar.selectionBackground": "#1e1e1e",
        "statusBar.background": "#1e1e1e",
        "statusBar.noFolderBackground": "#0A0A0D",
        "statusBar.debuggingBackground": "#511f1f"
    },
    "terminal.integrated.shell.linux": "/bin/zsh",
    "terminal.integrated.fontSize": 13,
    // "terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
    "terminal.integrated.shell.windows": "cmd.exe",
    "terminal.integrated.env.windows": {
        "CMDER_ROOT": "C:\\App\\Cmder"
    },
    "terminal.integrated.shellArgs.windows": [
        "/k",
        "C:\\App\\Cmder\\vendor\\init.bat"
    ],
    "files.autoSave": "afterDelay",
    "editor.wordWrap": "off",
    "breadcrumbs.enabled": true,
    "workbench.sideBar.location": "left",
    "debug.node.autoAttach": "on",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true
    },
    "editor.fontLigatures": true,
    "terminal.explorerKind": "external",
    "workbench.iconTheme": "material-icon-theme",
    // "http.proxy": "http://127.0.0.1:1080",
    // "http.proxyStrictSSL": false
}